{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["es2018"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "removeComments": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "outDir": "dist",
    "noEmit": true
  }
}
